{
    "name": "api-platform/pdg",
    "description": "PHP Documentation Generator",
    "authors": [
        {
            "name": "soyuka",
            "email": "soyuka@users.noreply.github.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "ApiPlatform\\PDGBundle\\Tests\\": "tests",
            "ApiPlatform\\PDGBundle\\": "src/",
            "App\\": "tests/Application/src"
        }
    },
    "bin": ["bin/pdg"],
    "require-dev": {
        "symfony/dom-crawler": "^6.2",
        "symfony/serializer": "^6.2",
        "api-platform/core": "^3.0",
        "phpunit/phpunit": "^9.5",
        "symfony/phpunit-bridge": "^6.2",
        "doctrine/annotations": "^2.0",
        "doctrine/doctrine-bundle": "^2.8",
        "doctrine/orm": "^2.14",
        "phpdocumentor/reflection-docblock": "^5.3",
        "symfony/asset": "^6.2",
        "symfony/dotenv": "^6.2",
        "symfony/expression-language": "^6.2",
        "symfony/property-access": "^6.2",
        "symfony/property-info": "^6.2",
        "symfony/security-bundle": "^6.2",
        "symfony/validator": "^6.2",
        "symfony/yaml": "^6.2"
    },
    "require": {
        "nelmio/cors-bundle": "^2.2",
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "phpstan/phpdoc-parser": "^1.15",
        "nikic/php-parser": "^4.15",
        "symfony/runtime": "^6.2",
        "symfony/console": "^6.2",
        "symfony/http-kernel": "^6.2",
        "symfony/browser-kit": "^6.2",
        "symfony/css-selector": "^6.2",
        "symfony/http-client": "^6.2",
        "symfony/framework-bundle": "^6.2",
        "spatie/yaml-front-matter": "^2.0",
        "dama/doctrine-test-bundle": "^7.1",
        "justinrainbow/json-schema": "^5.2",
        "zenstruck/foundry": "^1.27"
    },
    "config": {
        "allow-plugins": {
            "symfony/runtime": true
        }
    }
}
